{
  "name": "app",
  "version": "1.0.0",
  "private": true,
  "repository": "mcansh/covidstandardtime",
  "license": "MIT",
  "author": "Logan McAnsh <logan@mcan.sh> (https://mcan.sh)",
  "sideEffects": false,
  "main": "n/a",
  "scripts": {
    "dev": "pm2-dev pm2.config.js",
    "prebuild": "npm run clean",
    "build": "run-s build:*",
    "build:css": "postcss styles --base styles --dir app/styles --env production",
    "build:remix": "remix build",
    "clean": "del .cache server/build public/build app/styles",
    "start": "arc sandbox",
    "type-check": "tsc --project app",
    "lint": "eslint --cache --ext .js,.ts,.tsx ./",
    "test": "jest --passWithNoTests",
    "format": "prettier --write \"**/*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|graphql|mdx)\"",
    "validate": "npm-run-all --parallel lint format type-check test",
    "prepare": "husky install"
  },
  "dependencies": {
    "@architect/functions": "3.13.11",
    "@remix-run/architect": "0.17.0",
    "@remix-run/node": "0.17.0",
    "@remix-run/react": "0.17.0",
    "@remix-run/serve": "0.17.0",
    "@sentry/node": "6.3.5",
    "@sentry/react": "6.3.5",
    "@sentry/tracing": "6.3.5",
    "aws-sdk": "2.903.0",
    "date-fns": "2.21.2",
    "date-fns-tz": "1.1.4",
    "fathom-client": "3.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router": "*",
    "react-router-dom": "*",
    "remix": "0.17.0",
    "remix-utils": "1.0.2",
    "type-fest": "1.1.0"
  },
  "devDependencies": {
    "@mcansh/eslint-config": "3.0.1",
    "@remix-run/dev": "0.17.0",
    "@testing-library/jest-dom": "5.12.0",
    "@testing-library/react": "11.2.6",
    "@types/jest": "26.0.23",
    "@types/node": "15.0.2",
    "@types/react": "17.0.5",
    "@types/react-dom": "17.0.3",
    "@typescript-eslint/eslint-plugin": "4.22.1",
    "@typescript-eslint/parser": "4.22.1",
    "@zeit/next-source-maps": "0.0.4-canary.1",
    "autoprefixer": "10.2.5",
    "del-cli": "3.0.1",
    "eslint": "7.25.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-kentcdodds": "18.0.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.3.6",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-you-dont-need-lodash-underscore": "6.11.0",
    "husky": "6.0.0",
    "jest": "26.6.3",
    "lint-staged": "11.0.0",
    "npm-run-all": "4.1.5",
    "pm2": "4.5.6",
    "postcss": "8.2.14",
    "postcss-cli": "8.3.1",
    "prettier": "2.2.1",
    "shell-quote": "1.7.2",
    "tailwindcss": "2.1.2",
    "typescript": "4.2.4"
  },
  "engines": {
    "node": ">=14"
  }
}
